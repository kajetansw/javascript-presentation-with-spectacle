describe('Avengers Enrollment Form', () => {
  beforeEach(() => {
    cy.visit('http://localhost:8080/avengers/enroll');
  });

  it('should submit a form', () => {
    cy.get('.enroll-form')
      .find('.name')
      .type('Wade Wilson');
      .submit()
      .next()
      .should('contain', 'Your form has been submitted!');
  });
});